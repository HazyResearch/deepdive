
  deepdive.db.default {
    driver: "org.postgresql.Driver"
    url: "jdbc:postgresql://"${PGHOST}":"${PGPORT}"/"${DBNAME}
    user: ${PGUSER}
    password: ${PGPASSWORD}
    dbname: ${DBNAME}
    host: ${PGHOST}
    port: ${PGPORT}
    incremental_mode: MERGE
    }
    


      deepdive.schema.variables {
        
      }
    

      deepdive.extraction.extractors.ext_sentences {
        sql: """ TRUNCATE sentences;
        INSERT INTO sentences SELECT R0.document_id, R0.sentence, R0.words, R0.lemma, R0.pos_tags, R0.dependencies, R0.ner_tags, R0.sentence_offset, R0.sentence_id
FROM dd_new_sentences R0
        
        """
          output_relation: "sentences"
        style: "sql_extractor"
          
          input_relations: [
            dd_new_sentences
          ]
      }
    

      deepdive.extraction.extractors.ext_has_spouse_candidates {
        sql: """ TRUNCATE has_spouse_candidates;
        INSERT INTO has_spouse_candidates SELECT R0.person1_id, R0.person2_id, R0.sentence_id, R0.description, R0.relation_id, R0.is_true
FROM dd_new_has_spouse_candidates R0
        
        """
          output_relation: "has_spouse_candidates"
        style: "sql_extractor"
          
          input_relations: [
            dd_new_has_spouse_candidates
          ]
      }
    

      deepdive.extraction.extractors.ext_articles {
        sql: """ TRUNCATE articles;
        INSERT INTO articles SELECT R0.article_id, R0.text
FROM dd_new_articles R0
        
        """
          output_relation: "articles"
        style: "sql_extractor"
          
          input_relations: [
            dd_new_articles
          ]
      }
    

      deepdive.extraction.extractors.ext_has_spouse_features {
        sql: """ TRUNCATE has_spouse_features;
        INSERT INTO has_spouse_features SELECT R0.relation_id, R0.feature
FROM dd_new_has_spouse_features R0
        
        """
          output_relation: "has_spouse_features"
        style: "sql_extractor"
          
          input_relations: [
            dd_new_has_spouse_features
          ]
      }
    

      deepdive.extraction.extractors.ext_people_mentions {
        sql: """ TRUNCATE people_mentions;
        INSERT INTO people_mentions SELECT R0.sentence_id, R0.start_position, R0.length, R0.text, R0.mention_id
FROM dd_new_people_mentions R0
        
        """
          output_relation: "people_mentions"
        style: "sql_extractor"
          
          input_relations: [
            dd_new_people_mentions
          ]
      }
    
deepdive.pipeline.run: ${PIPELINE}
deepdive.pipeline.pipelines.extraction: [
  ext_sentences
  ext_people_mentions
  ext_has_spouse_candidates
  ext_has_spouse_features
  ext_articles
]
deepdive.pipeline.pipelines.endtoend: [
  ext_sentences
  ext_people_mentions
  ext_has_spouse_candidates
  ext_has_spouse_features
  ext_articles
]
