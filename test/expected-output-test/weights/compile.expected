deepdive.inference.factors.inf_istrue_a {
input_query: """
SELECT dd_id_0.dd_id AS "a.R0.dd_id"
     , R0.k AS "a.R0.k"
     , R0.k + R1.p AS "dd_weight_column_0"
FROM a R0
   , b R1
   , dd_variables_a AS dd_id_0
WHERE R1.k = R0.k
  AND R0.k = dd_id_0.k
"""
function: """Imply(a.R0.dd_label)"""
weight: """?(dd_weight_column_0)"""
input_relations: [
  a
  b
]

}

