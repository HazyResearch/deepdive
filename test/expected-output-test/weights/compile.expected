deepdive.schema.variables {
f.dd_label: Boolean
c.dd_label: Boolean
d.dd_label: Boolean
a.dd_label: Boolean
e.dd_label: Boolean

}

deepdive.inference.factors.inf_istrue_a {
input_query: """
SELECT R0.dd_id AS "a.R0.dd_id"
     , R0.k AS "a.R0.k"
     , R0.k + R1.p AS "dd_weight_column_0"
FROM a R0
   , b R1
WHERE R1.k = R0.k
"""
function: """Imply(a.R0.dd_label)"""
weight: """?(dd_weight_column_0)"""
input_relations: [
  a
  b
]

}

deepdive.inference.factors.inf_istrue_c {
input_query: """
SELECT R0.dd_id AS "c.R0.dd_id"
     , R0.k AS "c.R0.k"
FROM c R0
   , b R1
WHERE R1.k = R0.k
"""
function: """Imply(c.R0.dd_label)"""
weight: """?"""
input_relations: [
  c
  b
]

}

deepdive.inference.factors.inf_istrue_d {
input_query: """
SELECT R0.dd_id AS "d.R0.dd_id"
     , R0.k AS "d.R0.k"
     , R0.k AS "dd_weight_column_0"
     , R1.p AS "dd_weight_column_1"
FROM d R0
   , b R1
WHERE R1.k = R0.k
"""
function: """Imply(d.R0.dd_label)"""
weight: """?(dd_weight_column_0, dd_weight_column_1)"""
input_relations: [
  d
  b
]

}

deepdive.inference.factors.inf_istrue_e {
input_query: """
SELECT R0.dd_id AS "e.R0.dd_id"
     , R0.k AS "e.R0.k"
FROM e R0
   , b R1
WHERE R1.k = R0.k
"""
function: """Imply(e.R0.dd_label)"""
weight: """-10"""
input_relations: [
  e
  b
]

}

deepdive.inference.factors.inf_istrue_f {
input_query: """
SELECT R0.dd_id AS "f.R0.dd_id"
     , R0.k AS "f.R0.k"
FROM f R0
   , b R1
WHERE R1.k = R0.k
"""
function: """Imply(f.R0.dd_label)"""
weight: """-0.3"""
input_relations: [
  f
  b
]

}

deepdive.pipeline.run: ${PIPELINE}

deepdive.pipeline.pipelines.inference: [
  inf_istrue_a
  inf_istrue_c
  inf_istrue_d
  inf_istrue_e
  inf_istrue_f
]

deepdive.pipeline.pipelines.endtoend: [
  inf_istrue_a
  inf_istrue_c
  inf_istrue_d
  inf_istrue_e
  inf_istrue_f
]

