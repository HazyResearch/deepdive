output_relation: T___2
udf: """/bin/false"""
input_batch_size: ${INPUT_BATCH_SIZE}
input: """SELECT R0.a AS "R.R0.a", R0.b AS "R.R0.b"
FROM R R0
        WHERE 100 < R0.a AND R0.a < 200"""

}

deepdive.extraction.extractors.ext_S_0_by_f {
parallelism: ${PARALLELISM}
input_relations: [
  R
]
style: tsv_extractor
output_relation: S_0
udf: """/bin/false"""
input_batch_size: ${INPUT_BATCH_SIZE}
